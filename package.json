{
  "name": "@bouzuya/mr-jums",
  "description": "A client for blog.bouzuya.net using Cycle.js",
  "version": "0.9.1",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "babel": {
    "presets": [
      "es2015",
      "power-assert"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/mr-jums/issues"
  },
  "dependencies": {
    "@bouzuya/cyclejs-history-driver": "0.2.1",
    "@cycle/dom": "^18.3.0",
    "@cycle/http": "^14.3.0",
    "@cycle/run": "^3.2.0",
    "compression": "1.6.2",
    "express": "4.15.0",
    "fake-history-fns": "0.1.3",
    "fs-extra": "^4.0.1",
    "htmlescape": "1.1.1",
    "isomorphic-fetch": "2.2.1",
    "morgan": "1.8.1",
    "snabbdom-to-html": "3.1.1",
    "spa-town": "0.1.2",
    "xstream": "10.3.0"
  },
  "devDependencies": {
    "@types/compression": "0.0.33",
    "@types/express": "4.0.34",
    "@types/fs-extra": "^4.0.0",
    "@types/htmlescape": "1.1.0",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/morgan": "1.7.32",
    "@types/node": "6.0.52",
    "@types/power-assert": "1.4.29",
    "@types/proxyquire": "^1.3.28",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "6.3.2",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-power-assert": "^1.0.0",
    "beater": "^5.0.1",
    "less": "2.7.2",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "proxyquire": "^2.1.0",
    "rev-hash": "1.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^7.2.2",
    "source-map-loader": "0.1.6",
    "tap-dot-b": "^0.1.0",
    "typescript": "^2.4.2",
    "watch": "1.0.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  },
  "files": [
    ".tmp/es5/src/",
    "public/scripts/index-*.js",
    "public/styles/index-*.css",
    "rev.json"
  ],
  "homepage": "https://github.com/bouzuya/mr-jums",
  "keywords": [],
  "license": "MIT",
  "main": ".tmp/es5/src/cli/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/bouzuya/mr-jums.git"
  },
  "scripts": {
    "build": "npm-run-all -s 'build:ts' 'build:es2015' -p 'build:less'",
    "build:bundle": "webpack",
    "build:es2015": "babel --quiet .tmp/es2015/ -d .tmp/es5/",
    "build:less": "lessc --no-js --strict-imports --strict-math=on --strict-units=on src/common/view/index.less ./public/styles/index.css",
    "build:rev": "node scripts/rev.js",
    "build:ts": "tsc",
    "clean": "rimraf .tmp public",
    "prepublish": "npm-run-all -s 'clean' 'build' 'build:bundle' 'build:rev'",
    "start": "node .tmp/es2015/src/server/index.js",
    "test": "node .tmp/es5/test/index.js | tap-dot-b",
    "watch": "npm-run-all -p 'watch:*'",
    "watch:es2015": "npm run 'build:es2015' -- --watch",
    "watch:less": "watch --wait 2 'npm run build:less' 'src/common/view/'",
    "watch:server": "webpack-dev-server --config webpack.watch.config.js",
    "watch:test": "watch --wait 2 'npm test' '.tmp/es5/'",
    "watch:ts": "npm run 'build:ts' -- --watch"
  }
}
