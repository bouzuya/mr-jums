{
  "name": "mr-jums",
  "description": "A client for blog.bouzuya.net using Cycle.js",
  "version": "0.1.0",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "beater": {
    "dir": "./.tmp/es5/test/",
    "require": [
      "./scripts/enable-power-assert.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/mr-jums/issues"
  },
  "dependencies": {
    "@bouzuya/cyclejs-history-driver": "0.2.1",
    "@cycle/dom": "15.2.0",
    "@cycle/http": "12.0.0",
    "@cycle/run": "1.0.0",
    "express": "4.15.0",
    "isomorphic-fetch": "2.2.1",
    "morgan": "1.8.1",
    "snabbdom-to-html": "3.1.1",
    "xstream": "10.3.0"
  },
  "devDependencies": {
    "@types/compression": "0.0.33",
    "@types/express": "4.0.34",
    "@types/isomorphic-fetch": "0.0.33",
    "@types/morgan": "1.7.32",
    "@types/node": "6.0.52",
    "@types/power-assert": "1.4.29",
    "@types/proxyquire": "1.3.27",
    "@types/sinon": "1.16.33",
    "babel-cli": "6.23.0",
    "babel-loader": "6.3.2",
    "babel-preset-es2015": "6.22.0",
    "beater": "0.4.1",
    "beater-cli": "0.2.4",
    "compression": "1.6.2",
    "cpx": "1.5.0",
    "espower-loader": "1.2.0",
    "less": "2.7.2",
    "npm-run-all": "4.0.2",
    "power-assert": "1.4.2",
    "proxyquire": "1.7.11",
    "rimraf": "2.6.1",
    "sinon": "1.17.7",
    "source-map-loader": "0.1.6",
    "typescript": "2.2.1",
    "watch": "1.0.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  },
  "engines": {
    "node": "6.9.2",
    "npm": "3.10.9"
  },
  "homepage": "https://github.com/bouzuya/mr-jums",
  "keywords": [],
  "license": "MIT",
  "main": ".tmp/es2015/src/server/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/bouzuya/mr-jums.git"
  },
  "scripts": {
    "build": "npm-run-all -s 'build:ts' 'build:es2015' -p 'build:less' 'copy:*'",
    "build:bundle": "webpack",
    "build:es2015": "babel .tmp/es2015/ -d .tmp/es5/",
    "build:less": "lessc --no-js --strict-imports --strict-math=on --strict-units=on src/common/view/index.less ./public/index.css",
    "build:ts": "tsc",
    "clean": "rimraf .tmp public/index.css",
    "copy:images": "cpx 'src/images/**/*' 'public/images/'",
    "prepublish": "npm-run-all -s 'clean' 'build'",
    "server": "node .tmp/es2015/src/server/index.js",
    "start": "npm-run-all -s 'clean' 'build' 'build:bundle' 'server'",
    "test": "beater",
    "watch": "npm-run-all -p 'watch:*'",
    "watch:es2015": "npm run 'build:es2015' -- --watch",
    "watch:less": "watch --wait 2 'npm run build:less' 'src/common/view/'",
    "watch:server": "webpack-dev-server --config webpack.watch.config.js",
    "watch:test": "watch --wait 2 'npm test' '.tmp/es5/'",
    "watch:ts": "npm run 'build:ts' -- --watch"
  }
}
